// Configurations
support-for-ie = false
vendor-prefixes = webkit moz ms official

// Colors
$color-default = #505050
$color-accent = #2ebaae
$color-grey = #999
color-header = #000
color-header-sub = #a5a6a6
color-strong = #333
$color-bd = #ddd
$color-bd-input = #ccc
$color-bd-input-focus = #666
$color-bd-lite = #eee
$color-link = $color-default
$color-bg = #fff
$color-bg-primary = #6190e8
$color-bg-orange = #e4861f
color-bg-divider = #25292c
color-text = #777
color-text-label = #444
color-text-input = #ccc
color-text-input-focus = #666
color-twitter = #00aced
color-facebook = #3b5998
color-pinterest = #cb2027
color-google = #dd4b39
$color-theme = #6190e8
$gainsboro    = #eee
$color-btn-bg = #3d4348
$color-btn-bg-inverse = #fff
$color-btn-bg-lite = #43434e
$color-btn-text = #fff
$color-btn-text-inverse = #4696e5
$color-btn-text-active = #333
$color-btn-bghover = #313338



// Fonts
$font-family-monospace = "Input Mono", "PT Mono", Consolas, Monaco, Menlo, monospace
font-family = "Montserrat", sans-serif
font-family-btn = "Montserrat"
font-sans = "Helvetica Neue", Helvetica, Arial, sans-serif
font-serif = Georgia, "Times New Roman", serif
font-mono = "Source Code Pro", Consolas, Monaco, Menlo, Consolas, monospace
font-icon = FontAwesome
font-icon-path = "fonts/fontawesome-webfont"
font-icon-version = "4.0.3"
font-size = 14px
line-height = 1.5
line-height-title = 1.1em


$fa-var-twitter = "\f099"
$fa-var-twitter-square =  "\f081"
$fa-var-rss =  "\f09e"
$fa-var-github =  "\f09b"
$fa-var-github-alt =  "\f113";

// Header
$banner-bg := #434D63
$banner-color := #d0e4f8

sidebar = hexo-config("sidebar")

// Layout
block-margin = 50px
article-padding = 20px
mobile-nav-width = 280px
main-column = 9
sidebar-column = 3

if sidebar and sidebar isnt bottom
  _sidebar-column = sidebar-column
else
  _sidebar-column = 0

// Grids
column-width = 80px
gutter-width = 20px
columns = main-column + _sidebar-column

// Media queries
mq-mobile = "screen and (max-width: 479px)"
mq-tablet = "screen and (min-width: 480px) and (max-width: 767px)"
mq-normal = "screen and (min-width: 768px)"

// bgimg
$bgimg-body = "data:image/png;base64,"